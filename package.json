{
  "private": true,
  "version": "0.0.1",
  "description": "444 charts",
  "scripts": {
    "build": "cross-env NODE_ENV=production lerna run build --stream",
    "build:watch": "lerna run build:watch --stream --no-sort --concurrency 99",
    "lint": "lerna run lint",
    "sense": "lerna run --stream sense",
    "sense:watch": "lerna run --stream --no-sort --concurrency 99 sense:watch",
    "specs": "node scripts/generate-specs.js",
    "test": "npm run test:unit",
    "test:rendering": "aw puppet --glob 'test/rendering/**/*.spec.js' --mocha.bail false",
    "test:rendering:ci": "node ./scripts/rendering-test.js",
    "test:unit": "jest",
    "version": "yarn specs && git add **/*/api-specifications"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@after-work.js/aw": "6.0.14",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@nebula.js/cli": "2.1.0",
    "@nebula.js/cli-build": "2.1.0",
    "@nebula.js/cli-sense": "2.1.0",
    "@nebula.js/cli-serve": "2.1.0",
    "@testing-library/jest-dom": "5.14.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.3.1",
    "babel-loader": "8.2.3",
    "chai": "4.3.4",
    "enigma.js": "2.7.3",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "extend": "3.0.2",
    "fs-extra": "10.0.0",
    "html-webpack-plugin": "5.4.0",
    "jest": "27.3.1",
    "jest-canvas-mock": "2.3.1",
    "jest-circus": "27.3.1",
    "jest-junit": "13.0.0",
    "jsdoc-to-markdown": "7.1.0",
    "lerna": "4.0.0",
    "mocha-junit-reporter": "2.0.2",
    "prettier": "2.4.1",
    "scriptappy-from-jsdoc": "0.7.0",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0",
    "webpack": "5.59.1",
    "webpack-dev-server": "3.11.2"
